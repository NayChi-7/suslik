void dll_copy (loc r) {
  let x = *r;
  if (x == 0) {
  } else {
    let vx = *x;
    let w = *(x + 1);
    let a = *(x + 2);
    *r = w;
    dll_copy(r);
    let y1 = *r;
    if (y1 == 0) {
      let y = malloc(3);
      *r = y;
      *(y + 1) = 0;
      *(y + 2) = a;
      *y = vx;
    } else {
      let v = *y1;
      let y = malloc(3);
      *(y1 + 2) = y;
      *r = y;
      *(y + 1) = y1;
      *(y + 2) = a;
      *y1 = vx;
      *y = v;
    }
  }
}
